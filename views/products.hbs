<!DOCTYPE html>
<html>
<head>
  <title>product details</title>
</head>
<link rel="stylesheet" href="products.css">
<script src="abc.js" async></script>
<body>
    <section class="container content-section">
            <h2 class="section-header">Shirts Models</h2>
            <div class="shop-items">
                <div class="shop-item">
                    <span class="title">casual shirt</span>
                    <img class="image" src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcSn-updW2HLRvzwmZFvmyrL7YW0k1KoBSUkyfnbCnpaSI3sb2xh&usqp=CAU">
                    <div class="shop-item-details">
                        <span class="price">$100</span>
                        
                        <button class="btn btn-primary addtocart" type="button">ADD TO CART</button>
                    </div>
                </div>
                <div class="shop-item">
                    <span class="title">formal shirt</span>
                    <img class="image" src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcS6lkBR9yfg5jTe3WgkNLLtDJeYUchhugJ11noWGo3Eq7Sg37Du&usqp=CAU">
                    <div class="shop-item-details">
                        <span class="price">$50</span>
                        
                        <button class="btn btn-primary addtocart"type="button">ADD TO CART</button>
                    </div>
                </div>
                <div class="shop-item">
                    <span class="title">plain shirt</span>
                    <img class="image" src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcTuli_qjddUjl_H4s2erCODnke__3B2uIsBqffGh0eqjWYMgdz0&usqp=CAU">
                    <div class="shop-item-details">
                        <span class="price">$90</span>
                        
                        <button class="btn btn-primary addtocart" type="button">ADD TO CART</button>
                    </div>
                </div>
                <div class="shop-item">
                    <span class="title">check shirt</span>
                    <img class="image" src="data:image/jpeg;base64,">
                    <div class="shop-item-details">
                        <span class="price">$25</span>
                        
                        <button class="btn btn-primary addtocart" type="button">ADD TO CART</button>
                    </div>
                </div>
            </div>
        </section>
        <section class="container content-section">
            <h2 class="section-header">CART-ORDER DETAILS</h2>
            <div class="cart-row">
                <span class="cart-item cart-header cart-column">Item</span>
                <span class="cart-price cart-header cart-column">Price</span>
                <span class="cart-quantity cart-header cart-column">Quantity</span>
            </div>
            <div class="cart-items">
                <div class="cart-row">
                    <div class="cart-item cart-column">
                        <img class="cart-item-image" src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcS6lkBR9yfg5jTe3WgkNLLtDJeYUchhugJ11noWGo3Eq7Sg37Du&usqp=CAU" width="100" height="100">
                        <span class="cart-item-title">shirt</span>
                    </div>
                    <span class="cart-price cart-column">$50</span>
                    <div class="cart-quantity cart-column">
                        <input class="cart-quantity" type="number" value="1">
                        <button class="btn remove" type="button">REMOVE</button>
                    </div>
                </div>
            </div>
            <div class="cart-total">
                <strong class="cart-total-title">Total</strong>
                <span class="cart-total-price">$50</span>
            </div>
            <button class="btn btn-primary purchase" type="button">Purchase</button>
        </section>
</body>

</html>
